<div>
	<% if (flash) { %>
    	<% if (flash['success']) { %>
    		<div class="alert alert-success" role="alert"><%= flash['success'] %></div>
    	<% } else if (flash['error']) { %>
    		<div class="alert alert-danger" role="alert"><%= flash['error'] %></div>
		<% } else if (flash['warning']) { %>
    		<div class="alert alert-warning" role="alert"><%= flash[2] %></div>
    	<% } %>
    <% } %>
</div>